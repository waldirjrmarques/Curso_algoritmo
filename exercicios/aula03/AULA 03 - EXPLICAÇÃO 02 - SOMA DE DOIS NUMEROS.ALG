algoritmo "AULA 03 - EXPLICAÇÃO 02 - SOMA DE DOIS NUMEROS"
var
      NUMERO1 , NUMERO2 , SOMA : REAL
inicio
       ESCREVA ("DIGITE O 1° NUMERO: ")
       LEIA (NUMERO1)
       ESCREVA ("DOGITE O 2° NUMERO: ")
       LEIA (NUMERO2)
       
       SOMA <- NUMERO1 + NUMERO2
       
       ESCREVA ("A SOMA DOS DOIS NUMEROS É ", SOMA)
fimalgoritmo