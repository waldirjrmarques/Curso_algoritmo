Algoritmo "AULA 06 - EXERCICIO 02 - CONVERSOR DE REAL PARA DOLAR "

Var
// Seção de Declarações das variáveis 
     REAIS , DOLAR :REAL

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
      ESCREVA ("QUANTOS REAIS DESEJA CONVERTER ")
      LEIA (REAIS)
      
      DOLAR <- REAIS / 2.22
      
      ESCREVA ("R$:",REAIS," CONVERTIDO EM DOLAR USD:",DOLAR)

Fimalgoritmo