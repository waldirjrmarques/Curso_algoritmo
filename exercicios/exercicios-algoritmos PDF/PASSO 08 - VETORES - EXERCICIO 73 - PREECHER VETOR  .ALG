Algoritmo "PASSO 08 - VETORES - EXERCICIO 73 - PREECHER VETOR  "

Var
   // Seção de Declarações das variáveis

   POSICAO: vetor[0..9] de INTEIRO
   P,SUB: INTEIRO
Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   SUB <- 10
   PARA  P <- 0 ATE 9 PASSO 1  FACA
      SUB <- SUB - 1
      POSICAO[P] <- SUB
      ESCREVAL (POSICAO[P])
   FIMPARA


Fimalgoritmo


