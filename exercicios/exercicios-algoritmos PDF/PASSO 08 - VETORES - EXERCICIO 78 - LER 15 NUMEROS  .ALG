algoritmo "PASSO 08 - VETORES - EXERCICIO 78 - LER 15 NUMEROS  "

var
Vet: vetor[1..15] de INTEIRO
C: inteiro
inicio

PARA C <- 1 ATE 15 FACA
ESCREVA ("INFORME UM NUMERO:")
LEIA  (VET[C])
FIMPARA

para C <- 1 ate 15 faca

   se (Vet[C] % 10 = 0) entao
      escrevaL ("NA POSIÇÃO ",C,"[",Vet[C]:1,"]")
   senao
      escrevaL (Vet[C])
   Fimse
Fimpara


fimalgoritmo
