algoritmo "PASSO 08 - VETORES - EXERCICIO 79 - LER 10 NUMEROS MOSTRA PARES  "

var
Vet: vetor[1..10] de INTEIRO
C: inteiro
inicio

PARA C <- 1 ATE 10 FACA
ESCREVA ("INFORME ",C,"° NUMERO:")
LEIA  (VET[C])
FIMPARA

ESCREVAL ("NUMEROS PARES")
para C <- 1 ate 10 faca
   se (Vet[C] % 2 = 0) entao
      escrevaL ("NA POSIÇÃO ",C,"[",Vet[C]:1,"]")
   Fimse
Fimpara


fimalgoritmo
